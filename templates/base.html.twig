<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<title>
			{% block title %}Fitness-Club
			{% endblock %}
		</title>
		<link
		rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			<link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css" integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous"/><link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-0evHe/X+R7YkIZDRvuzKMRqM+OrBnVFBL6DOitfPri4tjfHxaWutUpFmBp4vmVor" crossorigin="anonymous">
			<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
			<link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet"/>
			<style>
				@import url('https://fonts.googleapis.com/css2?family=Exo + 2 &family=Roboto:wght@300&family=Satisfy&display=swap');
			</style>
			<link rel="stylesheet" href="{{ asset('styles/app.css') }}">

			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			<script src="https://code.jquery.com/jquery-3.6.1.min.js" integrity="sha256-o88AwQnZB+VDvE9tvIXrMQaPlFFSUTR+nldQm1LuPXQ=" crossorigin="anonymous"></script>

			<script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.3/dist/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
			<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
			<script src="https://gitcdn.github.io/bootstrap-toggle/2.2.2/js/bootstrap-toggle.min.js"></script>
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		{% if app.user %}
			<div class="container-fluid" id="navbar_font">
				<nav class="navbar navbar-expand-lg navbar-light bg-light">
					<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>

					<div class="collapse navbar-collapse" id="navbarSupportedContent">


						{% if is_granted('ROLE_ADMIN')  %}

							<ul class="navbar-nav mr-auto" id="navbar-font">
								<li class="nav-item">
									<a class="nav-link" href="/admin/partners/index">Les Partenaires</a>
								</li>
								<li class=" nav-item">
									<a class="nav-link" href="/admin/structures/index">Les Structures</a>
								</li>
								<li class="nav-item">
									<a class="nav-link" href="/admin/users/index">Les utilisateurs</a>
								</li>
								<li class="nav-item">
									<a class="nav-link" href="/admin/permissions/index">Les permissions</a>
								</li>
							{% endif %}
							{% if is_granted('ROLE_STRUCTURE') %}
								<ul class="navbar-nav mr-auto" id="navbar-font">
									<li class="nav-item">
										{% for structure in structures %}
											{% if structure.email == app.user.email %}
												<a class="nav-link" href="{{ path('app_structure', {'id': structure.id}) }}">Votre fiche Structure</a>
											{% endif %}
										{% endfor %}
									</li>
								</ul>
							{% endif %}
							{% if is_granted('ROLE_PARTNER') %}
								<ul class="navbar-nav mr-auto" id="navbar-font">
									<li class="nav-item">
										{% for partner in partners %}

											{% if partner.email == app.user.email %}
												<a class="nav-link" href="{{ path('app_partner', {'id': partner.id}) }}">Votre fiche Partenaire</a>
											{% endif %}
										{% endfor %}
									</li>
								</ul>
							{% endif %}
						</ul>
						<ul class="navbar-nav mr-auto" id="deconnect_link_mail">
							<li class="nav-item">
								{{ app.user.userIdentifier }}
								<a href="{{ path('app_logout') }}" id="deconnect_link">Déconnexion</a>
							</li>
						</ul>
					</div>
				{% endif %}

			</nav>
		</body>
	</body>
</html></body></html></div>{% block body %}{% endblock %}<footer><div class="container-fluid d-flex flex-wrap justify-content-center" id="footer">Fitness Club Copyright | 2022</div></footer></body></html></body></html></body></html>
