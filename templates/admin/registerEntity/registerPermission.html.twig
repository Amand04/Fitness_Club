{% extends 'base.html.twig' %}
{% block title %}Les Permissions
{% endblock %}
{% block body %}
	<section class="container my-3">
		<div class="row">
			<div class="col">
				<h3>Enregistrer une Permission</h3>
				<fieldset>
					{{ form_start(form) }}
					{{ form_widget(form) }}
					<button type="submit" class="btn btn-primary btn-lg my-3" id="btn_login">Enregistrer</button>
				</div>
				{{ form_end(form) }}
			</fieldset>
		</div>
	</div>
{% endblock %}
